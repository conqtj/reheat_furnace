FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    billet
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    walls
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    burner
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    exhaust
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
}


