FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      CO2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    billet
    {
        type            zeroGradient;
    }
    exhaust
    {
        type            zeroGradient;
    }
    burner_2
    {
        type            fixedValue;
        value           uniform 0.1;
    }
    burner_1
    {
        type            fixedValue;
        value           uniform 0.1;
    }
}


