FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.012;

boundaryField
{
    walls
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    billet
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    exhaust
    {
        /*
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
        */
        type            zeroGradient;
    }
    burner_2
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.1;
        value           $internalField;
    }
    burner_1
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.1;
        value           $internalField;
    }
}


