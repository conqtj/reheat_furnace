FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (8 0 0);

boundaryField
{
    exhaust
    {
        type            zeroGradient;
    }

    topWall
    {
        type            noSlip;
    }

    bottomWall
    {
        type            noSlip;
    }

    sideWalls
    {
        type            noSlip;
    }

    "burnerTile.*"
    {
        type            noSlip;
    }

    "burnerA.*"
    {
        type            swirlFlowRateInletVelocity;
        flowRate        constant 0.15;
        rpm             constant 0.15*0.3*2000*11.7891205781/(0.7*28+0.2*44+0.1*18);
        value           uniform (0 0 0);
	}

    "burnerB.*"
    {
        type            swirlFlowRateInletVelocity;
        flowRate        constant 0.25;
        rpm             constant 0.25*0.3*2000*11.7891205781/(0.7*28+0.2*44+0.1*18);
        value           uniform (0 0 0);
	}
}


