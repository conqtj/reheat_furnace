FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{

    bottomWall
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
	}

    sideWalls
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
	}

    topWall
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
	}

    "burnerTile.*"
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
	}

    "burnerA.*"
    {
        type            fixedValue;
        value           uniform 0.01;
    }

    "burnerB.*"
    {
        type            fixedValue;
        value           uniform 0.01;
    }

    exhaust
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
